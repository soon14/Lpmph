    	//"到店支付"按钮点击事件方法
    	function addTips(obj){
    		var value=$(obj).children("span").html();
    		if(value=="到店支付"){
    			//新增提示弹框
    			eDialog.alert('到店支付需要您亲自到出版社付款，才能发货！');
    		}
			if(value=="银行转账"){
				//新增银行转账提示弹框
    			eDialog.alert('在办理汇款时，请您务必在电汇单上的用途栏注明订单号，方便我们及时查询发货！有问题请及时联系客服，客服电话：4008989100');
			}
    	}
        $(function () {
            $('.toggle-hd').click(function () {
                $(this).parents(".bill-toggle").find(".toggle-bd").toggle();
            });

            U.tab('#teamTab', '#teamTabc');

            $('#delbill').click(function(){
                U.alertInfo("您确定要删除该发票信息吗？");
            });

            $('#modzttm').click(function(){
                bDialog.open({
                    title : "自提时间",
                    width : 540,
                    height : 300,
                    url : 'payztTime_dialog.html'
                });
            });
            $('.sendmd a').click(function(){
                if($(this).hasClass('zt')){
                    $('.zt-list').show();
                }else{
                    $('.zt-list').hide();
                }
            });


            $('.sl-addr').live('hover',function(event){
					if(event.type == 'mouseenter'){
						$(this).addClass('hover');
					}else{
						$(this).removeClass('hover');
					}
            });

			$('.order-switch a').click(function () {
                $(this).siblings().removeClass('checked')
                    .find('input[type="radio"]').attr('checked', false);
                $(this).addClass('checked')
                    .find('input[type="radio"]').attr('checked', true);
			});
        });

    </script>
