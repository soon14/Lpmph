<div class="seller-well" style="padding: 10px;">
	    <!--多店铺-->
    <div id="multShop" class="seller-body" style="display:none;margin-top:0;">
        <div class="seller-tit seller-font">我的店铺</div>
		<input type="hidden" id="shopId" value=""/>
        <div class="shop-scroll">
            <ul class="seller-list clearfix" id="shopList">
                
            </ul>
            <div class="slide-page clearfix">
                <ul id="shopSlideList">
                    <li>
                        <a class="active" href="javascript:"></a>
                    </li>
                </ul>
            </div>
        </div>
    </div>
	<div class="shopAcctList-wrap">
			<div class="shopAcctList-tit">
				<p class="shopAcctList-txt">我的资产</p>
			</div>
			<div class="shopAcctList-showCount">
				<div class="counter-tit-money">
					<p>账户总资产（元）</p>
					<p id="total"></p>
				</div>
				<div class="counter-tit-money">
					<p>&nbsp;</p>
					<p>=</p>
				</div>
				<div class="counter-tit-money">
					<p>可用余额<span class="span-counter-iconyu">&nbsp;</span></p>
					<p id="balance"></p>
				</div>
				<div class="counter-tit-money">
					<p>&nbsp;</p>
					<p>+</p>
				</div>
				<div class="counter-tit-money">
					<p>不可用余额<span class="span-counter-iconyu">&nbsp;</span></p>
					<p id="frozen"></p>
				</div>
			</div>
			<div class="shopAcctList-detailBtn">
				<div class="shopAcctList-charts">
					<p id="per"></p>
				</div>
				<div class="shopAcctList-balance">
					<p>可用余额</p>
					<p id="balance2"></p>
				</div>
				<div class="shopAcctList-wdr-btn">
					<!-- 标准，可用状态类名 class="wdr-btn-ok"  不可用状态 类名 class="wdr-btn-no" 根据条件显示所需要的-->
					<a id="button1" href="javascript:void(0);"  class="wdr-btn-ok"  style="color: #ffffff;">申请提现</a>
					
					<a id="button2" href="javascript:void(0);" class="wdr-btn-showDetail" style="color: #ffffff;display:none">查看提现申请进度</a>
					
					<a id="button3" href="javascript:void(0);"  class="wdr-btn-showDetail" style="color: #ffffff;background-color:#ff6666;border:1px solid #ff6666;display:none">撤销提现</a>
					<br>
					<!--<a href="javascript:viod(0);" class="wdr-btn-no" style=" color: #ffffff;">申请提现</a>-->
				</div>
			</div>
		</div>
		<div class="shopAcctList-payments">
			<p class="shopAcctList-txt">收支明细</p>
			<div class="payments-header">
				<a href="javascript:void(0);" class="payments-curMonth" style=" color: #ffffff;">当前月</a>
				结算月账单：<span id="money"></span>
				<a href="javascript:void(0);" class="payments-prexMonth">上个月</a>
				结算月账单：<span id="lastMonth"></span>
			</div>
			<div class="select-btn">
				<div class="calendar-box demo-box"></div>
			</div>
		</div>


