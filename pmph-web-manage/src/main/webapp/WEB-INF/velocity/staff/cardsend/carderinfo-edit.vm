#parse("/module/mod-form.vm")

<script type="text/javascript" src="$!{BUSIJS_PATH}/staff/cardsend/carderinfo-edit.js?version=$!{JS_VERSION}" ></script>

<div class="formBox row-fluid">
    <form id="p_detailInfoForm" class="form-horizontal required-validate " style="margin-bottom: 0px;">
        <input type="hidden" name="id" id="id" value="$!{infodto.id}" />
        <div class="span6">
            <div class="row-fluid formSep">
                <div class="span8">
                    <label class="control-label">发卡人姓名：</label>
                    <div class="controls">
                        <input type="text" class="input-block-level htmlcheck required" name="disName" value="$!{infodto.disName}" maxlength="32" />
                    </div>
                </div>
            </div>
            <div class="row-fluid formSep">
                <div class="span8">
                    <label class="control-label">联系电话：</label>
                    <div class="controls">
                        <input type="text" class="input-block-level checkPhone htmlcheck required" name="disMobile" value="$!{infodto.disMobile}" maxlength="11" />
                    </div>
                </div>
            </div>
            <div class="span8">
                <div class="pull-right">
                    <button type="button" class="btn btn-info" id="p_btnFormSave"><i class="icon-ok-sign"></i> 保存</button>
                    <button type="button" class="btn" id="p_btnReturn"><i class="icon-reply"></i> 返回</button>
                </div>
            </div>
        </div>
        <div class="clear"></div>
    </form>
</div>

	